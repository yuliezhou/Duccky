page{
  background:url('http://oi2f6lsr3.bkt.clouddn.com/bg.png') repeat-y;
  font-family: "苹方";
  width: 100%;
}
scroll-view{
   white-space: nowrap;
   width: 100%;
}
.w96{
  width: 96%;
  margin-left: 2%;
  margin-right: 2%;
}
.container{
  color: #fff;
  width: 100%;
}
.top{
  width: 100%;
  min-height: 670rpx;
  margin-bottom: 20rpx;
  color: #ffe9c5;
  background-image: url('http://oi2f6lsr3.bkt.clouddn.com/%E9%A6%96%E5%B1%8Fbg.png');
  background-size: 100% 100%;
}
.top image{
  display: inline-block;
  width:19rpx;
  height: 28rpx; 
  margin-left: 5rpx;
}
.area{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 40rpx;
}
.area_set{
  width: 200rpx;
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  font-size: 28rpx;
}
.area_set image{
  width: 30rpx;
  height: 30rpx;
}
.area_text{
  display: flex;
  align-items: center;
  margin-right: 30rpx;
  font-size: 28rpx;
}
.area_text image{
  width: 20rpx;
  height: 36rpx;
  margin: 0;
  margin-right:10rpx; 
}
.area_text image.add{
  margin: 0;
  margin-left: 20rpx;
  width: 30rpx;
height: 30rpx;
}

.area_text text{
  display: inline-block;
  margin-left:10rpx; 
}
navigator{
  display: block;
  padding-left: 20rpx;
  padding-top: 20rpx;
}

.swiper{
    width: 100%;
}
.temp_swiper{height: 560rpx;}
.lunar_calendar{
  margin-bottom: 20rpx; 
}
.animal{
  font-size: 30rpx;
  margin-left: 10rpx;
}
.temp{
  font-size: 36rpx;
  text-align: center;
  margin-top: 10rpx 
}
.temp_top{
  font-size: 180rpx;
  margin-bottom: 30rpx;
  text-align: center;
  width: 80%;
  margin: 0 auto;
}

.temp_top .middle{
  display: inline-block;
}
.temp_middle{
  margin-bottom: 20rpx;
}
.temp_main{
  margin-bottom: 40rpx;
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.temp_main image{
    width: 60rpx;
    height: 60rpx;
    margin-right:20rpx; 
    vertical-align: bottom;
}
.animals{
  width: 387rpx;
  height: 48rpx;
  margin: 0 auto;
  color: #fdb632;
  line-height: 48rpx;
  font-size: 26rpx;
  background-image: url('http://oi2f6lsr3.bkt.clouddn.com/%E5%B9%B4%E4%BB%BD%E8%BE%B9%E6%A1%86.png');
  background-size: 100% 100%;
}
.time{
  color: #666;
}
.weather_day,.weather_week,.hl{
  background-image:url("http://oi2f6lsr3.bkt.clouddn.com/%E4%B8%AD%E9%97%B4bg%E6%96%B0.png");
  background-repeat: repeat-x;
  background-size: 100% 100%;
  position: relative;
  font-size: 28rpx;
  font-weight: normal;
}
.bg_left{
  background:url("http://oi2f6lsr3.bkt.clouddn.com/100px%E5%B7%A6.png") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  height: 100%;
  top: 0;
  left: 0;
  width: 50rpx;
}
.bg_right{
  background:url("http://oi2f6lsr3.bkt.clouddn.com/100px%E5%8F%B3.png") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  height: 100%;
  top: 0;
  right: 0;
  width: 50rpx;
}
.weather_day{
  width: 100%;
  color: #353535;
  height: 200rpx;
  margin-bottom: 20rpx;
}
.weather_week{
  width: 100%;
  height: 236rpx;
  color: #353535;
  margin-bottom: 20rpx;
  background-image:url("http://oi2f6lsr3.bkt.clouddn.com/zhongjian.png");
  background-repeat: repeat-x;
  background-size: 100% 100%;
}
.weather_week .bg_left{
  background-image: url('http://oi2f6lsr3.bkt.clouddn.com/zuo.png') 
}
.weather_week .bg_right{
  background-image: url('http://oi2f6lsr3.bkt.clouddn.com/you.png') 
}
.day{
  width: 124rpx;
  display: inline-block;
  height: 200rpx;
  padding-top: 20rpx; 
  text-align: center;
}
.week{
  display: inline-block;
  width: 120rpx;
  margin-right: 20rpx;
  height: 236rpx;
  padding-top: 40rpx;
  padding-left: 10rpx; 
  text-align: center;
}
.day_temp,.week_temp{
  margin-left: 8rpx;
}
.week_temp{
  display: block;
}
.low{
  color: #666;
}
.high{
  margin-bottom:10rpx; 
}
.week image,.day image{
  margin: 10rpx auto;
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
}
.hl{
  width: 100%;
  height: 200rpx;
  margin-bottom: 20rpx;
  display: table;
}
.hl_box{
  height: 160rpx;
  margin: 20rpx 0;
  display: table-cell;
  vertical-align: middle;
  font-weight: 600;
}
.weather_should,.weather_not{
  width: 96%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
 
  position: relative;
}
.weather_should{
  margin-bottom: 10rpx;
}
.weather_should scroll-view,.weather_not scroll-view{
  margin-left:130rpx;
  width: 590rpx;  
}
.should,.not{
  padding: 0 10rpx;
  display: inline-block;
  vertical-align: bottom;
  text-align: left;
  font-size: 28rpx;
  font-weight: normal;
}
.should{
  color: #1dadea;
 
}
.not{
  color: #e83c2e;
   vertical-align: middle;
}
image{
  display: block;
  margin: 10rpx;
}
.hl image{
  width: 54rpx;
  height: 54rpx;
  display: inline-block;
  position: absolute;
  left: 30rpx;
  top: 50%;
  margin-top:-27rpx;
  vertical-align: middle;
}
.else{
  height: 180rpx;

}
.right{
  margin-right: 10rpx;
}
.else view{
  height: 180rpx;
  font-size: 30rpx;
  font-weight: 600; 
  text-align: center;
  width: 375rpx;
  display: inline-block;
  position: relative;

}
.else view text:last-child{
  display: block;
}
.else view text{
  color: #bf9f6e;

  display: block;
  font-weight:normal;
  font-size: 24rpx;
}
.else view text:first-child{
   font-weight:800;
   font-size: 28rpx;
}
.else view.text_box{
  display: inline-block;
  width: 220rpx;
  height: 68rpx;
  text-align: right;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -100rpx;
  margin-top: -34rpx;
}

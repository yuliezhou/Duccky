.data_top{
	width: 100%;
	height: 424rpx;
	background: #594E60;
	margin-bottom: 20rpx;
}
.data_left{
	margin-left: 30rpx;
}
.data_left_item{
	height: 108rpx;
	position: relative;
	width: 250rpx;
	padding: 27rpx 0;
	padding-left: 30rpx;
}
.data_left_item::after{
	content: '';
	position: absolute;
	height: 108rpx;
	top: 27rpx;
	left: 0;
	width: 10rpx;
	background: linear-gradient(130deg,rgba(255,213,106,1) 0%,rgba(255,170,47,1) 100%);
	border-radius: 6rpx;
}
.data_left_title{
	font-size: 29rpx;
	color: #fff;
	opacity: 0.7;
}
.data_left_number{
	color: #fff;
	font-size: 53rpx;
	font-weight: bold;
}
.data_right{
	position: relative;
	margin-right: 64rpx;
}
.data_backimg{
	width: 348rpx;
	height: 348rpx;
}
.data_ab{
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
}
.data_title{
	font-size: 28rpx;
	color: #fff;
}
.data_number{
	color: #FFBD5B;
	font-size: 76rpx;
	width: 100%;
	text-align: center;
	margin-top: 10rpx;
	margin-bottom: 4rpx;
	font-weight: bold;
	background-image: linear-gradient(#FFD56A,#FFAA2F);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;	
}
.data_icon{
	width: 16rpx;
	height: 20rpx;
	margin-bottom: 8rpx;
}
.data_english{
	font-size: 22rpx;
	color: #fff;
}
.data_list{
	margin-bottom: 20rpx;
}
.list_title{
	border-bottom: 1px solid #FAFAFA;
	height: 70rpx;
	width: 100%;
	background: #fff;
}
.list_content{
	border-bottom: 1px solid #FAFAFA;
	height: 98rpx;
	width: 100%;
	background: #fff;
}
.list_title_item{
	width: 20%;
	text-align: center;
	color: #4C4252;
	font-size: 26rpx;
	line-height: 70rpx;
	height: 70rpx;
}

.list_content_item{
	width: 20%;
	text-align: center;
	font-size: 24rpx;
	line-height: 70rpx;
	height: 70rpx;	
	color: #919191;
}